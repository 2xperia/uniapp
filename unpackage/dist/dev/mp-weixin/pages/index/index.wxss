
.content {
	display: flex;
	flex-direction: column;
}
.logo {
	height: 150rpx;
	width: 300rpx;
}
.scan{
	height: 45rpx;
	width: 45rpx;
}
.down{
	height: 45rpx;
	width: 45rpx;
	margin-left: auto;
}
.text-area {
	border-left-style:solid;
	border-left-width: 12rpx;
	border-left-color: red;
	margin-left: 10rpx;
}
.title {
	font-size: 36rpx;
	margin-left: 6rpx;
	color: black;
	font-weight: 700;
}
.imagebox{
	height: 150rpx;
	width: 100%;
	margin-right: auto;
	margin-bottom: 40rpx;
	background-color: rgba(243,243,243);
}
.form{
	width: 93%;
}
.input{
	margin-left: auto;
	text-align:right;
	padding-right: 10rpx;
}
.cell{
	display: flex;
	margin: 10px;
	margin-top: 30rpx;
	width: 100%;
	height:70rpx;
	border-bottom-style:solid;
	border-bottom-width: 4rpx;
	border-bottom-color: #d7d7d7;
}
.titleText{
	font-weight: 600;
}
.button{
	margin-right: 0px;
	margin-top:-3px;	
	width: 100px;
	height:30px;	
	/* margin-bottom: 12px;
	padding-top: 6px; */
	font-size: 15px;
	background-color: #2C405A;
	color: #4CD964;
	text-align: center;
}
.choose{
	display: flex;
	margin-top: 50rpx;
}
.submit1{
	width: 150px;
	height:50px;
	background-color: #ffd8c1;
	font-weight: 500;
	color: #aa5500;
}
.submit2{
	width: 150px;
	height:50px;
	background-color: #d90000;
	font-weight: 500;
	color: white;
}
.box{
	display: flex;
	margin-top: 50rpx;
	/* text-align: center; */
}
.back{
	height: 90rpx;
	width: 90rpx;
	background-color: #e9e9e9;
	border-radius: 30px;
	margin: 0 auto;
	padding: 5rpx;
}
.downimage{
	height: 70rpx;	
	width: 70rpx;
	margin: 5px;
}
.boxs{
	width: 50%;
	display: block;
	text-align: center;
	justify-content: center;
	align-items: center;
}
.text{
	margin-top: 10px;
	font-weight: 600;
}

